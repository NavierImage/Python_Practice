test_case = int(input())
result = []
for i in range(test_case):
    a = list(input().split())
    a_new = float(a[0])
    for j in a:
        if j == '@':
            a_new = a_new * 3
        elif j =='%':
            a_new = a_new + 5
        elif j == '#':
            a_new = a_new - 7
        else:
            pass
    result.append(a_new)
for i in range(len(result)):
    print('%.2f' % result[i])
    # mars math
    # problem: https://www.acmicpc.net/problem/5355
